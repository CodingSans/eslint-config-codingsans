{
  "name": "@codingsans/eslint-config",
  "version": "5.1.0",
  "description": "CodingSans Eslint config",
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "lint": "eslint '*.js'",
    "check-generated-config": "eslint -c index.js --print-config typescript-recommended.js > generated.typescript-recommended.json"
  },
  "author": "LaTotty <latotty@codingsans.com>",
  "contributors": [
    "LaTotty <latotty@codingsans.com>",
    "afgTheCat <fgabor@codingsans.com>"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": "CodingSans/eslint-config-codingsans",
  "bugs": {
    "url": "https://github.com/CodingSans/eslint-config-codingsans/issues"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0"
  },
  "peerDependencies": {
    "eslint": "^6.0.0 || ^7.0.0",
    "prettier": "^2.1.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.51",
    "eslint": "^7.11.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
