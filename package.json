{
  "name": "@codingsans/eslint-config",
  "version": "6.0.0-rc",
  "description": "CodingSans Eslint config",
  "license": "MIT",
  "repository": "CodingSans/eslint-config-codingsans",
  "bugs": {
    "url": "https://github.com/CodingSans/eslint-config-codingsans/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "author": "LaTotty <latotty@codingsans.com>",
  "contributors": [
    "LaTotty <latotty@codingsans.com>",
    "afgTheCat <fgabor@codingsans.com>"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "files": [
    "*.js",
    "*.json"
  ],
  "scripts": {
    "lint": "eslint '*.js'",
    "check-generated-config": "eslint -c index.js --print-config typescript-recommended.js > generated.typescript-recommended.json"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.7.1",
    "eslint-config-next": "^12.0.0",
    "@angular-eslint/eslint-plugin": ">=12.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.14.3",
    "eslint": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.0"
  }
}
